{
  "name": "ReactNativeBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android:dev": "react-native run-android --variant=devDebug",
    "android:prod": "react-native run-android --variant=prodDebug",
    "android:staging": "react-native run-android --variant=stagingDebug",
    "build:android:dev": "cd android && ./gradlew assembleDevRelease && cd ..",
    "build:android:prod": "cd android && ./gradlew assembleProdRelease && cd ..",
    "build:android:staging": "cd android && ./gradlew assembleStagingRelease && cd ..",
    "build:android:dev:bundle": "cd android && ./gradlew :app:bundleDevRelease && cd ..",
    "build:android:prod:bundle": "cd android && ./gradlew :app:bundleProdRelease && cd ..",
    "build:android:staging:bundle": "cd android && ./gradlew :app:bundleStagingRelease && cd ..",
    "ios:dev": "yarn ios --scheme \"ReactNativeBoilerplateDev\"",
    "ios:prod": "yarn ios --scheme \"ReactNativeBoilerplateProd\"",
    "ios:staging": "yarn ios --scheme \"ReactNativeBoilerplateStaging\""
  },
  "dependencies": {
    "@eva-design/eva": "^2.1.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@react-navigation/stack": "^6.1.1",
    "@ui-kitten/components": "^5.1.1",
    "axios": "^0.26.0",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "query-string": "^7.1.1",
    "react": "17.0.2",
    "react-native": "0.67.3",
    "react-native-config": "^1.4.5",
    "react-native-device-info": "^8.4.9",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-safe-area-context": "^4.1.2",
    "react-native-screens": "^3.13.0",
    "react-native-svg": "^12.1.1",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reduxsauce": "^1.2.1",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/runtime": "^7.17.2",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "metro-react-native-babel-preset": "^0.68.0",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
